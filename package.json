{
  "name": "@zhengxs/common-algorithm",
  "version": "0.0.1-0",
  "description": "常用算法，支持 nodejs 与浏览器",
  "keywords": [
    "tree",
    "descartes",
    "algorithm"
  ],
  "main": "dist/lib.commonjs.js",
  "browser": "dist/lib.umd.js",
  "module": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -cw rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:docs": "jsdoc -c jsdoc.json",
    "test": "jest",
    "cov": "jest --coverage",
    "report": "",
    "lint": "eslint \"{{src,test}/**/*,*}.js\"",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags origin master",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-html-reporter": "^2.8.2",
    "jsdoc": "^3.6.3",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.15",
    "power-assert": "^1.6.1",
    "prettier": "^1.19.1",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-visualizer": "^3.3.2"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 5"
  },
  "homepage": "https://github.com/zhengxs2018/common-algorithm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhengxs2018/common-algorithm.git"
  },
  "bugs": {
    "url": "https://github.com/zhengxs2018/common-algorithm/issues"
  },
  "author": {
    "name": "zhengxs2018",
    "email": "zhengxs2018@foxmail.com"
  },
  "license": "MIT"
}
